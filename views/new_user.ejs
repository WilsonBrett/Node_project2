<% include ./partials/header %>
<section class="section_back">
	<form class="form" id="user_form" name="user_form" action="/users" method="post">
		<h2>New User Registration</h2>
		<!--<label for="email" form="user_form">Enter your email:</label>-->
		<input type="text" id="email_box" name="email" placeholder="Email" required>
		<input type="text" id="password_box" name="password" placeholder="Password" required>
		<input type="text" id="location_box" name="location" placeholder="City" required>
	
		<div>
			<input type="submit" id="submit_user_btn" name="submit_user" value="Submit" required>
			<input type="submit" id="cancel_user_btn" name="cancel_user" value="Cancel" required>
		</div>
	</form>
</section>
<% include ./partials/footer %>
